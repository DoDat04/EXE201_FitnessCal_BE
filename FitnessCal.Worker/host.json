{
  "version": "2.0",
  "logging": {
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "excludedTypes": "Request"
      }
    },
    "logLevel": {
      "default": "Information",
      "Microsoft": "Warning",
      "Worker": "Information",
      "Function": "Information",
      "Host.Results": "Information",
      "Host.Aggregator": "Information"
    }
  },
  "extensions": {
    "timers": {
      "scheduleMonitor": {
        // ‚öôÔ∏è Cho ph√©p Azure l∆∞u l·∫°i l·ªãch s·ª≠ th·ª±c thi timer ƒë·ªÉ tr√°nh ch·∫°y tr√πng
        "enabled": true
      },
      "batchSize": 1,
      "maxDegreeOfParallelism": 1,
      "maxConcurrentInvocations": 1
    }
  },
  "extensionBundle": {
    "id": "Microsoft.Azure.Functions.ExtensionBundle",
    "version": "[4.*, 5.0.0)"
  },
  "functionTimeout": "00:10:00",
  "healthMonitor": {
    // ü©∫ Theo d√µi t√¨nh tr·∫°ng function worker
    "enabled": true,
    "healthCheckInterval": "00:00:10",
    "healthCheckWindow": "00:02:00",
    "healthCheckThreshold": 6
  }
}
